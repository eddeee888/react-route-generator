{
  "private": true,
  "name": "@route-codegen/sample",
  "version": "0.0.1",
  "scripts": {
    "pregenerate": "rm -rf outputs",
    "generate": "yarn generate:default && yarn generate:with-origins",
    "postgenerate": "yarn --silent format --loglevel silent",
    "generate:default": "route-codegen --stacktrace --verbose --config routegen.yml",
    "generate:with-origins": "PRIMARY_DOMAIN=domain.com route-codegen --stacktrace --verbose --config routegen-with-origins.yml",
    "format": "prettier --config .prettierrc --write \"**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "prettier": "2.0.5"
  }
}
