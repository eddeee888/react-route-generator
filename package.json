{
  "name": "route-codegen",
  "version": "0.0.4",
  "description": "Route generator written in Typscript for React Router, NextJS, NodeJS and more",
  "main": "./dist/index.js",
  "bin": {
    "route-codegen": "./dist/bin/route-codegen.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "generate": "node dist/tests/run.js",
    "test:cli": "rm -rf ./tests/output && npm run build && node dist/index.js --config tests/routegen.yml",
    "pub": "rm -rf ./dist && yarn build && yarn publish"
  },
  "keywords": [
    "react-router",
    "next-js",
    "route",
    "generator"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/js-yaml": "^3.12.2",
    "@types/node": "^13.1.8",
    "@types/yargs": "^15.0.3",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "js-yaml": "^3.13.1",
    "path-to-regexp": "^6.1.0",
    "yargs": "^15.1.0"
  }
}
