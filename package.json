{
  "name": "route-codegen",
  "version": "0.3.0",
  "description": "Route generator written in Typscript for React Router, NextJS, NodeJS and more",
  "homepage": "https://github.com/eddeee888/route-codegen",
  "bugs": "https://github.com/eddeee888/route-codegen/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddeee888/route-codegen.git"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": {
    "route-codegen": "./dist/bin/route-codegen.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "generate:sample": "rm -rf ./sample/output && yarn --silent build && node dist/bin/route-codegen.js --stacktrace --verbose --config sample/routegen.yml && yarn --silent prettier --config .prettierrc --write \"sample/**/*.{ts,tsx}\" --loglevel silent ",
    "build": "rm -rf ./dist && tsc",
    "format:prettier": "prettier --config .prettierrc --write \"src/**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/js-yaml": "^3.12.2",
    "@types/node": "^13.1.8",
    "@types/yargs": "^15.0.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "semver": "^7.1.3",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "js-yaml": "^3.13.1",
    "path-to-regexp": "^6.1.0",
    "yargs": "^15.1.0"
  },
  "keywords": [
    "react-router",
    "next-js",
    "route",
    "generator"
  ]
}
